{
  "name": "spovizz-api",
  "version": "4.0.0",
  "description": "SpoVizz - API",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://h2908577.stratoserver.net:7990/scm/vig/zenga.git"
  },
  "scripts": {
    "start": "node -r ./node_modules/dotenv-safe/config -r module-alias/register src/index.js dotenv_config_path=.env.prod",
    "start-prod-local": "node -r ./node_modules/dotenv-safe/config -r module-alias/register src/index.js dotenv_config_path=.env",
    "migrate:up": "node -r ./node_modules/dotenv-safe/config -r module-alias/register ./node_modules/migrate-mongo/bin/migrate-mongo up",
    "migrate:status": "node -r ./node_modules/dotenv-safe/config -r module-alias/register ./node_modules/migrate-mongo/bin/migrate-mongo status",
    "dev": "cross-env UV_THREADPOOL_SIZE=600 PINO_PRETTY=true nodemon --trace-warnings -r ./node_modules/dotenv-safe/config -r module-alias/register src/index.js dotenv_config_path=.env",
    "debug": "PINO_PRETTY=true nodemon --inspect -r ./node_modules/dotenv-safe/config src/index.js dotenv_config_path=.env",
    "puppeteer-run": "node pupeteer_test.js",
    "test": "node -r module-alias/register node_modules/.bin/jest --forceExit",
    "test:dyntimelines": "node -r ./node_modules/dotenv-safe/config -r module-alias/register ./node_modules/.bin/mocha --reporter=min ./test/unit/opta/dynamiccomp.test.js",
    "test:wise": "mocha --reporter=tap ./test/unit/wisehockey/**/*.test.js | tee report.txt",
    "test:cover": "nyc --reporter=text-summary mocha",
    "test:e2e": "npm run init:testdata && npm run test",
    "init:testdata": "npm run clean && npm run seed",
    "clean": "node scripts/seeder/run_clean.js",
    "seed": "node -r module-alias/register scripts/seeder/run_seed.js",
    "seed:templates": "npm run seed -- collections=['template_records']",
    "seed:setup": "npm run seed -- collections=['setup']",
    "seed:setup:adler": "npm run seed -- collections=['setup'] client='adler'",
    "seed:setup:demo": "npm run seed -- collections=['setup','users'] client=demo",
    "seed:setup:transfermarkt": "npm run seed -- collections=['setup','users'] client='transfermarkt'",
    "seed:setup:fcb": "npm run seed -- collections=['setup','users'] client='fcb'",
    "seed:setup:bild": "npm run seed -- collections=['setup','users'] client='bild'",
    "seed-players": "node scripts/seeder/seed_players.js",
    "eslint": "eslint --fix --config .eslintrc.json \"src/**/*.js\"",
    "remark": "remark . -e \"md,MD\"",
    "coverage": "npm run coverage:merge-report",
    "coverage:clean": "rm -rf .nyc_output && rm -rf coverage",
    "coverage:merge-report": "nyc report --reporter=lcov --reporter=text --reporter=json --temp-dir=./coverage/merged --report-dir=./coverage/merged",
    "load-test": "artillery run --environment local ./load-testing/local.yaml",
    "generate-user": "node generate-user.js pass='test'",
    "test-axios": "node -r module-alias/register ./test/axios_instance.js",
    "get-vods": "node -r module-alias/register ./scripts/performstats/get_vods.js",
    "get-olympic-vods": "node -r module-alias/register ./scripts/heimspiel/get_vods.js",
    "get-epg-list": "node -r module-alias/register ./scripts/performstats/generate_epg_file.js",
    "get-teams": "node -r module-alias/register ./scripts/performstats/get_team_ids.js",
    "get-competitions": "node -r module-alias/register ./scripts/performstats/get_competitions.js",
    "check-auto-update": "node -r module-alias/register ./src/controllers/utils/auto-updater.js",
    "sync-seasons": "node -r module-alias/register ./scripts/performstats/sync_seasons.js"
  },
  "nyc": {
    "reporter": [
      "json",
      "text",
      "lcov"
    ],
    "report-dir": "coverage/e2e",
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "**/data/**",
      "**/node_modules/**",
      "**/.history/**",
      "**/test/**",
      "**/coverage/**",
      "**/tmp/**",
      "_BASE/**"
    ],
    "all": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@bull-board/api": "5.23.0",
    "@bull-board/express": "5.23.0",
    "archiver": "^7.0.1",
    "async": "3.2.6",
    "async-es": "3.2.6",
    "axios": "^1.6.8",
    "base-64": "^1.0.0",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bullmq": "5.13.2",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.29.1",
    "date-fns-tz": "^1.3.7",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.10",
    "expeditious-engine-redis": "0.1.2",
    "express": "4.21.0",
    "express-expeditious": "5.1.1",
    "express-fileupload": "1.5.1",
    "express-validator": "7.2.0",
    "flat": "5.0.2",
    "fs-extra": "11.2.0",
    "helmet": "7.1.0",
    "http-status-codes": "2.3.0",
    "i18n": "0.15.1",
    "ioredis": "5.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "4.17.21",
    "migrate-mongo": "11.0.0",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.3",
    "mongoose": "8.3.2",
    "mongoose-paginate-v2": "1.8.0",
    "mongoose-sequence": "6.0.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.8.0",
    "nodemailer-mailgun-transport": "^2.1.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pino": "9.5.0",
    "pino-http": "10.3.0",
    "pino-loki": "2.3.1",
    "pino-pretty": "11.3.0",
    "puppeteer": "22.7.1",
    "request-ip": "^3.3.0",
    "sharp": "^0.33.3",
    "socket.io": "^4.7.5",
    "socket.io-emitter": "^3.2.0",
    "socket.io-redis": "^6.1.1",
    "swagger": "^0.7.5",
    "swagger-jsdoc": "^7.0.0-rc.4",
    "swagger-ui-express": "5.0.0",
    "tmp": "^0.2.3",
    "trim-request": "^1.0.6",
    "uuid": "^9.0.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@swc/core": "^1.7.0-nightly-20240714.1",
    "@swc/jest": "0.2.36",
    "axios-mock-adapter": "1.22.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "8.22.0",
    "eslint-config-formidable": "^4.0.0",
    "faker": "^5.5.3",
    "husky": "^8.0.1",
    "jest": "29.7.0",
    "json2csv": "^6.0.0-alpha.2",
    "lint-staged": "^13.0.3",
    "mocha": "^10.4.0",
    "mongo-seeding": "^3.7.2",
    "mongodb-memory-server": "^9.1.8",
    "nodemon": "^2.0.19",
    "nyc": "^15.1.0",
    "prettier": "3.2.5",
    "remark-cli": "^11.0.0"
  },
  "keywords": [
    "javascript",
    "api",
    "rest",
    "zenga",
    "webservice"
  ],
  "config": {
    "mongodbMemoryServer": {
      "debug": false
    }
  },
  "_moduleAliases": {
    "@": "./"
  }
}
